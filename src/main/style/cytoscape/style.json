[{
  "selector": "node[?group]",
  "style": {
    "shape": "round-rectangle",
    "width": 100,
    "height": 50,
    "background-opacity": 0.5,
    "z-index": 0
  }
}, {
  "selector": "node[?role]",
  "style": {
    "width": 25,
    "height": 25,
    "z-index": 1,
    "background-color": "#C4DBE0"
  }
}, {
  "selector": "node[?group]:orphan, node[?group]:orphan > node[?role]",
  "style": {
    "background-color": "#2EB5E0"
  }
}, {
  "selector": "node[?group]:child, node[?group]:child > node[?role]",
  "style": {
    "background-color": "#00A8A8"
  }
}, {
  "selector": "node[label]",
  "style": {
    "label": "data(label)"
  }
}, {
  "selector": "edge",
  "style": {
    "width": 2,
    "curve-style": "bezier",
    "line-color": "black",
    "label": "data(label)",
    "text-rotation": "autorotate",
    "text-valign": "bottom",
    "text-background-color": "white",
    "text-background-opacity": 1
  }
}, {
  "selector": "edge[arrow = 'compatibility']",
  "style": {
    "target-arrow-shape": "diamond",
    "target-arrow-color": "black",
    "line-style": "dashed"
  }
}, {
  "selector": "edge[arrow = 'extends']",
  "style": {
    "target-arrow-shape": "triangle",
    "target-arrow-fill": "hollow",
    "target-arrow-color": "black",
    "line-style": "solid"
  }
}, {
  "selector": "edge[?biDir]",
  "style": {
    "source-arrow-shape": "diamond",
    "source-arrow-color": "black"
  }
}, {
  "selector": "node[?goal]",
  "style": {
    "shape": "round-rectangle",
    "text-valign": "center",
    "text-halign": "center",
    "width": 100,
    "height": 50,
    "text-wrap": "wrap",
    "text-max-width": 90
  }
}, {
  "selector": "edge[relationType = 'and']",
  "style": {
    "target-arrow-shape": "triangle",
    "target-arrow-color": "black",
    "target-arrow-fill": "hollow",
    "line-style": "solid"
  }
}, {
  "selector": "edge[relationType = 'or']",
  "style": {
    "target-arrow-shape": "triangle",
    "target-arrow-color": "black",
    "target-arrow-fill": "hollow",
    "line-style": "dashed"
  }
}, {
  "selector": "edge[?dependency]",
  "style": {
    "target-arrow-shape": "vee",
    "target-arrow-color": "black",
    "line-style": "dotted",
    "label": "depends on"
  }
}]
